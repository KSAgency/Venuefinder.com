function createApplicationWindow(e,t,i,o,a,n,r,d,l,s,c){var u=Titanium.UI.createWindow({backgroundColor:o,title:i,width:Ti.UI.FILL,height:Ti.UI.FILL,zIndex:0});if("iPhone"==Ti.App.Properties.getString("osname")?(u.setBarColor("#2195be"),u.setBackButtonTitle("Back"),u.setTintColor("#FFF"),u.setTranslucent(!1),u.setTitleControl(Ti.UI.createLabel({text:i,color:"#FFF",width:160,height:25,textAlign:Ti.UI.TEXT_ALIGNMENT_CENTER}))):"iPad"==Ti.App.Properties.getString("osname")&&("forflipwindow"!=c?(u.setBarColor("#2195be"),u.setBackButtonTitle("Back"),u.setTintColor("#FFF"),u.setTranslucent(!1),u.setTitleControl(Ti.UI.createLabel({text:i,color:"#FFF",width:400,height:25,textAlign:Ti.UI.TEXT_ALIGNMENT_CENTER}))):(u.setNavBarHidden(!0),u.setFullscreen(!0))),null!=t){var f=t.toString();"[object TiUIView]"!=f&&"[object TiUIWindow]"!=f&&(f=f.substring(0,9))}if(""==s&&(s="info"),null!=l&&""!=l&&null!=t&&""!=t){var T=require("/views/"+t),m=T(e,l,s,u);u.add(m),e.activeTab.open(u)}else if("collview/"==f&&null!=t){var T=require("/views/"+t),m=T(e,u);null!=m&&u.add(m),u.open()}else if("children/"!=f&&null!=f)u.add(t),e.activeTab.open(u);else if(null!=t){Ti.API.info("view : "+t);var T=require("/views/"+t),m=T(e,u);u.add(m),e.activeTab.open(u)}var p=Titanium.UI.createWebView({html:'<html><head><script type="text/javascript" src="s_code.js"></script></head><body><script>s.channel="'+Ti.App.Properties.getString("osname")+'";s.pageName="'+a+'";s.prop1="'+n+'";s.prop2="'+r+'";s.prop3="'+d+'";s.events="event4";var s_code=s.t(); if(s_code)document.write(s_code)</script><script>if(navigator.appVersion.indexOf("MSIE")>=0)document.write(unescape("%3C")+"!-"+"-")</script><noscript><img src="https://cmpi.122.2o7.net/b/ss/cmpivenuefinderappprod/1/H.25.2--NS/0?[AQB]&cdp=3&[AQE]"height="1" width="1" border="0" alt="" /></noscript></body></html>',opacity:0,width:0,height:0,left:0,right:0,touchEnabled:!1});return u.add(p),null==t?u:void 0}module.exports=createApplicationWindow;