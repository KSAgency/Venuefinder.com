function newApplicationTabGroup(){var e,t=Ti.UI.createTabGroup({tintColor:"#FFF",tabsTintColor:"#2195be"}),i=require("/views/mapView"),a=require("/views/mediaView"),o=require("/views/offersView"),n=require("/views/favouritesView");e=require("iPad"===Ti.App.Properties.getString("osname")?"/views/searchView":"/views/searchViewOld");var r=require("/builders/applicationDashboard"),l=r(t),d=Titanium.UI.createButton({title:"Home",backgroundColor:"#FFF",tintColor:"#FFF"});e.home_button=d,i.home_button=d,o.home_button=d;var s=e(t,"Search","#d2e8f5"),u=i(t,"Map","#d2e8f5"),c=a(t,"Videos","#d2e8f5"),f=o(t,"Offers","#d2e8f5"),m=n(t,"Favourites","#d2e8f5"),g=Ti.UI.createTab({title:"Search",icon:"/images/search.png",window:s});t.tab1=g;var p=Ti.UI.createTab({title:"Map",icon:"/images/map.png",window:u});t.tab2=p;var h=Ti.UI.createTab({title:"Video",icon:"/images/media.png",window:c});t.tab3=h;var T=Ti.UI.createTab({title:"Offers",icon:"/images/offers.png",window:f});t.tab4=T;var b=Ti.UI.createTab({title:"Favourites",icon:"/images/favourites.png",window:m[0],bubbleParent:!1});return t.tab5=b,"Android"==Ti.App.Properties.getString("osname")&&(g.setIcon(null),p.setIcon(null),h.setIcon(null),T.setIcon(null),b.setIcon(null)),t.addTab(g),t.addTab(p),t.addTab(h),t.addTab(T),t.addTab(b),"Android"!=Titanium.App.Properties.getString("osname")&&(t.addEventListener("singletap",function(){m[1]()}),s.leftNavButton=d,u.leftNavButton=d,c.leftNavButton=d,f.leftNavButton=d,m[0].leftNavButton=d,d.addEventListener("click",function(){l.show()})),"Android"==Ti.App.Properties.getString("osname")?(t.addEventListener("androidback",function(){t.activeTab.open(l)}),[t]):[t,l]}module.exports=newApplicationTabGroup;