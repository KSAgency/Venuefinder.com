function newApplicationTabGroup(){var e=Ti.UI.createTabGroup({tintColor:"#FFF",tabsTintColor:"#2195be"}),t=require("/views/searchView"),i=require("/views/mapView"),a=require("/views/mediaView"),n=require("/views/offersView"),o=require("/views/favouritesView"),r=require("/builders/applicationDashboard"),d=r(e),l=t(e,"Search","#d2e8f5"),s=i(e,"Map","#d2e8f5"),u=a(e,"Videos","#d2e8f5"),c=n(e,"Offers","#d2e8f5"),f=o(e,"Favourites","#d2e8f5"),m=Ti.UI.createTab({title:"Search",icon:"/images/search.png",window:l});e.tab1=m;var p=Ti.UI.createTab({title:"Map",icon:"/images/map.png",window:s});e.tab2=p;var T=Ti.UI.createTab({title:"Video",icon:"/images/media.png",window:u});e.tab3=T;var g=Ti.UI.createTab({title:"Offers",icon:"/images/offers.png",window:c});e.tab4=g;var h=Ti.UI.createTab({title:"Favourites",icon:"/images/favourites.png",window:f[0],bubbleParent:!1});if(e.tab5=h,"Android"==Ti.App.Properties.getString("osname")&&(m.setIcon("/images/searchAndroid.png"),p.setIcon("/images/mapAndroid.png"),T.setIcon("/images/mediaAndroid.png"),g.setIcon("/images/offersAndroid.png"),h.setIcon("/images/favouritesAndroid.png")),e.addTab(m),e.addTab(p),e.addTab(T),e.addTab(g),e.addTab(h),"Android"!=Titanium.App.Properties.getString("osname")){e.addEventListener("singletap",function(){f[1]()});var b=Titanium.UI.createButton({title:"Home",backgroundColor:"#FFF",tintColor:"#FFF"});l.leftNavButton=b,s.leftNavButton=b,u.leftNavButton=b,c.leftNavButton=b,f[0].leftNavButton=b,b.addEventListener("click",function(){d.show()})}return"Android"==Ti.App.Properties.getString("osname")?(e.addEventListener("androidback",function(){e.activeTab.open(d)}),[e]):[e,d]}module.exports=newApplicationTabGroup;