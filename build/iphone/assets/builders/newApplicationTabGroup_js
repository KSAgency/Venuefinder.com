function newApplicationTabGroup(){var e=Ti.UI.createTabGroup({tintColor:"#FFF",tabsTintColor:"#2195be"}),t=require("/views/searchView"),i=require("/views/mapView"),a=require("/views/mediaView"),n=require("/views/offersView"),o=require("/views/favouritesView"),r=require("/builders/applicationDashboard"),l=r(e),d=Titanium.UI.createButton({title:"Home",backgroundColor:"#FFF",tintColor:"#FFF"});t.home_button=d,i.home_button=d,n.home_button=d;var s=t(e,"Search","#d2e8f5"),u=i(e,"Map","#d2e8f5"),c=a(e,"Videos","#d2e8f5"),f=n(e,"Offers","#d2e8f5"),m=o(e,"Favourites","#d2e8f5"),g=Ti.UI.createTab({title:"Search",icon:"/images/search.png",window:s});e.tab1=g;var p=Ti.UI.createTab({title:"Map",icon:"/images/map.png",window:u});e.tab2=p;var T=Ti.UI.createTab({title:"Video",icon:"/images/media.png",window:c});e.tab3=T;var h=Ti.UI.createTab({title:"Offers",icon:"/images/offers.png",window:f});e.tab4=h;var b=Ti.UI.createTab({title:"Favourites",icon:"/images/favourites.png",window:m[0],bubbleParent:!1});return e.tab5=b,"Android"==Ti.App.Properties.getString("osname")&&(g.setIcon("/images/searchAndroid.png"),p.setIcon("/images/mapAndroid.png"),T.setIcon("/images/mediaAndroid.png"),h.setIcon("/images/offersAndroid.png"),b.setIcon("/images/favouritesAndroid.png")),e.addTab(g),e.addTab(p),e.addTab(T),e.addTab(h),e.addTab(b),"Android"!=Titanium.App.Properties.getString("osname")&&(e.addEventListener("singletap",function(){m[1]()}),s.leftNavButton=d,u.leftNavButton=d,c.leftNavButton=d,f.leftNavButton=d,m[0].leftNavButton=d,d.addEventListener("click",function(){l.show()})),"Android"==Ti.App.Properties.getString("osname")?(e.addEventListener("androidback",function(){e.activeTab.open(l)}),[e]):[e,l]}module.exports=newApplicationTabGroup;