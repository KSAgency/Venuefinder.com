function freeResultsFormat(e,t,i,a){if("iPad"!=Ti.App.Properties.getString("osname"))var n=Titanium.UI.createTableViewRow({height:85,venueTitle:e,uniqueID:t,selectedBackgroundColor:"#FFF",backgroundColor:"#FFF"});else var n=Titanium.UI.createLabel({height:77,width:327,left:10,top:10,venueTitle:e,uniqueID:t});var o=Titanium.UI.createLabel({text:e,left:10,width:300,height:45,top:5,ellipsize:!0,color:"#2195be",font:{fontSize:18,fontWeight:"bold"},touchEnabled:!1}),r=Titanium.UI.createLabel({text:i+a,left:10,width:300,height:30,top:50,ellipsize:!0,color:"#A2BE1C",font:{fontSize:16},touchEnabled:!1});return"Android"!=Ti.App.Properties.getString("osname")&&(o.setLeft(15),r.setLeft(15)),e.length<20&&(o.setHeight(20),o.setTop(20),r.setTop(40)),n.add(o),n.add(r),n}module.exports=freeResultsFormat;