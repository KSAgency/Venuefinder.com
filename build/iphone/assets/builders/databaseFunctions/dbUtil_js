function getVenueForId(e){for(var t=createDatabase("/venuefinder.db","venuefinder"),i=t.execute('select * from Venue where VenueID = "'+e+'"'),a=[];i.isValidRow();)a.VenueID=i.fieldByName("VenueID"),a.VenueName=i.fieldByName("VenueName"),a.Town=i.fieldByName("Town"),a.BedroomsNo=null==i.fieldByName("BedroomsNo")?0:i.fieldByName("BedroomsNo"),a.Postcode=i.fieldByName("Postcode"),a.MeetingRoomsNo=null==i.fieldByName("MeetingRoomsNo")?0:i.fieldByName("MeetingRoomsNo"),a.PackageCode=i.fieldByName("PackageCode"),a.AddressLine1=i.fieldByName("AddressLine1"),a.AddressLine2=i.fieldByName("AddressLine2"),a.County=i.fieldByName("County"),a.Country=i.fieldByName("Country"),a.Tel=i.fieldByName("Tel"),a.Fax=i.fieldByName("Fax"),a.Email=i.fieldByName("Email"),a.URL=i.fieldByName("URL"),a.BookingURL=i.fieldByName("BookingURL"),a.MaxCateringCapacity=null==i.fieldByName("MaxCateringCapacity")?0:i.fieldByName("MaxCateringCapacity"),a.MaxMeetingCapacity=null==i.fieldByName("MaxMeetingCapacity")?0:i.fieldByName("MaxMeetingCapacity"),a.Latitude=i.fieldByName("Latitude"),a.Longitude=i.fieldByName("Longitude"),i.next();return t.close(),a}var createDatabase=require("/builders/databaseFunctions/createDatabase");module.exports.getVenueForId=getVenueForId;