function createCheckField(e){var t=Titanium.UI.createTableViewRow({color:"#399ad4",backgroundColor:"#f7f7f7",title:e,font:{fontFamily:"Arial",fontSize:"14",fontWeight:"bold"},hasCheck:!1,checkValue:0,height:"45",tintColor:"#2195be"});return t.addEventListener("click",function(){0==t.hasCheck?(t.setHasCheck(!0),t.checkValue=1):(t.setHasCheck(!1),t.checkValue=0)}),[t]}module.exports=createCheckField;