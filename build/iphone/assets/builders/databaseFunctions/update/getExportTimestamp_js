function getExportTimestamp(e){if(1==Titanium.Network.online){var t=Ti.Network.createHTTPClient();t.open("GET",e),t.setRequestHeader("Accept","application/xml"),t.send(),t.onload=function(){try{var e=this.responseXML.getElementsByTagName("dateTime").item(0).text;e=e.substr(0,10),e=e.replace("-",""),e=e.replace("-",""),e=e.replace("-","")}catch(t){var i=new Date;if(i.getMonth().toString()<9)var a="0"+(i.getMonth()+1).toString();else var a=(i.getMonth()+1).toString();if(i.getDate().toString()<10)var n="0"+i.getDate().toString();else var n=i.getDate().toString();var e=i.getFullYear().toString()+a+n}Ti.App.fireEvent("recievedDate",{date:e})}}}module.exports=getExportTimestamp;