function createImageGallery(e,t,i){var o=[],a=require("/builders/createApplicationWindow"),n=a(e,null,"","#000",null,null,null,null);"Android"!=Ti.App.Properties.getString("osname")&&(n.orientationModes=[Titanium.UI.LANDSCAPE_LEFT,Titanium.UI.LANDSCAPE_RIGHT],n.setOrientation(Titanium.UI.LANDSCAPE_LEFT));for(var r=0;r<t.length;r++){var d=Ti.UI.createScrollView({maxZoomScale:100,minZoomScale:1,width:Ti.UI.FILL,height:Ti.UI.FILL}),l=Titanium.UI.createImageView({image:t[r],defaultImage:"/images/icon.png"}),s=l.toImage();s.width>=s.height?(l.setWidth(Ti.UI.FILL),d.setContentWidth(Ti.UI.FILL),d.setContentHeight(s.height)):(l.setHeight(Ti.UI.FILL),d.setContentHeight(Ti.UI.FILL),d.setContentWidth(s.width)),d.add(l),o.push(d)}Ti.API.info(i);var u=Ti.UI.createScrollableView({views:o,currentPage:i,width:Ti.UI.FILL,height:Ti.UI.FILL});n.add(u),e.activeTab.open(n)}module.exports=createImageGallery;
