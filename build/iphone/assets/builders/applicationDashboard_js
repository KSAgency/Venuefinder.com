function applicationDashboard(e){var t=Titanium.UI.createWindow({title:"Dashboard",backgroundColor:"#fff",zIndex:"5",top:"0"}),i=require("/builders/databaseFunctions/update/createUpdateButton"),o=(i(t,!1),Titanium.UI.createView({top:-170,height:170,width:36,right:25,layout:"vertical",backgroundColor:"#666",zIndex:20})),a=Titanium.UI.createButton({backgroundImage:"/images/socialflag.png",right:25,height:60,width:37,top:0,zIndex:20}),n=Titanium.UI.createImageView({image:"/images/shadow_overlay.png",top:-20,right:25,height:60,width:37,zIndex:20});t.add(a),t.add(n),t.add(o);var r=Titanium.UI.createImageView({image:"/images/twitter.png",width:26,height:26,left:5,top:10}),d=Titanium.UI.createImageView({image:"/images/facebook.png",width:26,height:26,left:5,top:10}),l=Titanium.UI.createImageView({image:"/images/linkedin.png",width:26,height:26,left:5,top:10});o.add(d),o.add(r),o.add(l);var u=!0;a.addEventListener("click",function(){1==u?(a.animate({top:170,duration:700}),o.animate({top:0,duration:700}),u=!1):(a.animate({top:0,duration:700}),o.animate({top:-170,duration:700}),u=!0)}),n.addEventListener("click",function(){a.fireEvent("click")}),d.addEventListener("click",function(){Titanium.Platform.openURL("http://www.facebook.com/VenuefinderBlueGreen")}),r.addEventListener("click",function(){Titanium.Platform.openURL("http://twitter.com/venuefinder_com")}),l.addEventListener("click",function(){Titanium.Platform.openURL("http://www.linkedin.com/groups/venuefindercom-blue-green-4081928?gid=4081928&trk=hb_side_g")});var s=Titanium.UI.createImageView({image:"/images/logo.png",top:"35",width:"100%"});t.add(s);var c=[{title:"Venue Search",hasChild:!0,index:0,color:"#2195be",font:{fontSize:"20",fontWeight:"bold"}},{title:"Map",hasChild:!0,index:1,color:"#2195be",font:{fontSize:"20",fontWeight:"bold"}},{title:"Videos",hasChild:!0,index:2,color:"#2195be",font:{fontSize:"20",fontWeight:"bold"}},{title:"Special Offers",hasChild:!0,index:3,color:"#2195be",font:{fontSize:"20",fontWeight:"bold"}},{title:"My Favourites",hasChild:!0,index:4,color:"#2195be",font:{fontSize:"20",fontWeight:"bold"}},{title:"About venuefinder.com",hasChild:!0,index:5,color:"#2195be",font:{fontSize:"20",fontWeight:"bold"}}];if("iPad"!=Ti.App.Properties.getString("osname")){var f=Titanium.UI.createTableViewRow({touchEnabled:!1,height:"200",color:"#2195be",font:{fontSize:"20",fontWeight:"bold",selectedColor:"#FFF"}});c.push(f)}var T=Titanium.UI.createTableView({data:c,color:"#2195be",font:{fontSize:"20"},backgroundColor:"transparent",rowBackgroundColor:"#fff",top:"28%",width:"101%"});"android"==Ti.Platform.osname?(T.setBottom("60"),T.setTop(1==Titanium.Gesture.orientation?"28%":"48%")):T.setHeight("200%"),"iPad"==Ti.App.Properties.getString("osname")?(s.setWidth("600"),s.setTop("110"),s.setImage("/images/logoIpad.png"),T.setWidth("600"),T.setBorderRadius("10"),T.setHeight(Titanium.UI.SIZE),T.setTop("300"),o.setRight("220"),a.setRight("220")):"Android"==Ti.App.Properties.getString("osname")&&T.setWidth(275),t.add(T);var m=Titanium.UI.createWebView({background:"#d2e8f5",color:"#fff",top:44.5,url:"/about_us.html",height:Ti.UI.FILL,width:Ti.UI.FILL,zIndex:"10"});if("iPad"==Ti.App.Properties.getString("osname"))m.setUrl("/about_us_iPad.html");else if("Android"==Ti.App.Properties.getString("osname")){m.setScalesPageToFit(!0),m.setTop(0);var h=Titanium.UI.createWindow({title:"About venuefinder.com",backgroundColor:"#fff",zIndex:10,top:0});h.addEventListener("androidback",function(){1==m.canGoBack()?m.goBack():h.close()})}if("android"!=Ti.Platform.osname){var g=Titanium.UI.createButton({style:Titanium.UI.iPhone.SystemButtonStyle.DONE,backButtonTitle:"Back",title:"Back"}),p=Titanium.UI.iOS.createToolbar({items:[g],text:"About Venue Finder",barColor:"#2195be",backButtonTitle:"Back",backgroundColor:"#FFF",width:"100%",height:"20%",top:"0%"});g.addEventListener("click",function(){1==m.canGoBack()?m.goBack():(t.remove(m),"android"!=Ti.Platform.osname&&(t.remove(p),t.remove(g)))})}T.addEventListener("click",function(i){5==i.rowData.index?"android"!=Ti.Platform.osname?(t.add(p),t.add(m)):(h.add(m),h.open()):"android"==Ti.Platform.osname?(e.setActiveTab(i.rowData.index),t.close()):(e.setActiveTab(i.rowData.index),t.hide())});var b=require("/builders/createAdvert"),I=b();return t.add(I),Ti.Gesture.addEventListener("orientationchange",function(e){"android"==Ti.Platform.osname&&T.setTop(e.source.isPortrait()?"28%":"48%")}),t}module.exports=applicationDashboard;