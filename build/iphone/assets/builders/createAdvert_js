function createAdvert(e){if(1==Ti.Network.online)var t=Titanium.UI.createImageView({image:"/images/advert.png",bottom:"0",width:"320",height:"50",zIndex:"9"});else var t=Titanium.UI.createImageView({image:"/images/advert.png",bottom:"0",width:"320",height:"50",zIndex:"9"});return"iPad"==Ti.App.Properties.getString("osname")&&(t.setBottom(null!=e&&""!=e?e:"50"),t.setWidth("600"),t.setHeight("94")),t}module.exports=createAdvert;