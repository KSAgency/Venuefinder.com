function featuredResultsFormat(e,t,i,a,n,o,r,l,d,u){if(null!=u||"iPad"!=Ti.App.Properties.getString("osname"))var s=Titanium.UI.createTableViewRow({height:150,venueTitle:e,uniqueID:t,selectedBackgroundColor:"#FFF",backgroundColor:"#FFF"});else var s=Titanium.UI.createView({height:150,width:327,left:10,top:10,venueTitle:e,uniqueID:t,backgroundColor:"#FFF",borderRadius:10,borderColor:"#999"});var c=Titanium.UI.createView({width:96,left:5,height:87,top:10,touchEnabled:!1});"Android"!=Ti.App.Properties.getString("osname")&&c.setLeft(15);var f=Titanium.UI.createImageView({image:"http://www.venuefinder.com/adverts/"+i,defaultImage:"/images/icon.png",hires:!0,width:140,touchEnabled:!1});null==i&&f.setImage("/images/icon.png");var m=Titanium.UI.createActivityIndicator();"Android"!=Ti.App.Properties.getString("osname")&&m.setStyle(Titanium.UI.iPhone.ActivityIndicatorStyle.DARK),c.add(m),m.show(),f.addEventListener("load",function(){f.setWidth(280),m.hide()});var p=Titanium.UI.createImageView({image:"/images/featuredbanner.png",touchEnabled:!1,top:0,left:0});null!=d&&""!=d&&p.setImage(d),c.add(f),c.add(p),s.add(c);var T=Titanium.UI.createView({width:190,left:120,height:45,top:5,touchEnabled:!1}),g=Titanium.UI.createLabel({text:e,width:Ti.UI.SIZE,height:Ti.UI.SIZE,top:0,left:0,ellipsize:!0,color:"#2195be",font:{fontSize:18,fontWeight:"bold"},touchEnabled:!1});"Android"!=Ti.App.Properties.getString("osname")&&(T.setLeft(125),T.setWidth(180));var h=Titanium.UI.createLabel({text:r+l,left:120,width:190,height:30,top:43,ellipsize:!0,color:"#A2BE1C",font:{fontSize:16},touchEnabled:!1});"Android"!=Ti.App.Properties.getString("osname")&&(h.setLeft(125),h.setWidth(180)),(null==o||""==o)&&(o="No description could be retrieved for this venue.");var b=Titanium.UI.createLabel({text:o,left:120,width:190,height:35,top:65,touchEnabled:!1,color:"#666",font:{fontSize:14},touchEnabled:!1});"Android"!=Ti.App.Properties.getString("osname")&&(b.setLeft(125),b.setWidth(180)),e.length<20&&(g.setHeight(20),h.setTop(20),b.setTop(43),b.setHeight(55)),T.add(g),s.add(T),s.add(h),s.add(b);var I=Ti.UI.createView({height:25,width:Ti.UI.SIZE,top:110,left:0,layout:"horizontal",touchEnabled:!1});if("Android"!=Ti.App.Properties.getString("osname")&&I.setLeft(10),null!=a&&a>=0){var v=Titanium.UI.createImageView({image:"/images/has_bedrooms.png",height:25,width:25,left:5,touchEnabled:!1});I.add(v)}if(1==n){var A=Titanium.UI.createImageView({image:"/images/has_access.png",height:25,width:25,left:5,touchEnabled:!1});I.add(A)}var w=new Date,S=w.getDate(),E=w.getMonth()+1,U=w.getFullYear();10>S&&(S="0"+S),10>E&&(E="0"+E),w=U+"-"+E+"-"+S;var y=require("/builders/databaseFunctions/createDatabase"),F=y("/venuefinder.db","venuefinder"),C=F.execute('SELECT * FROM Offers WHERE EntryID="'+t+'" AND validToDate>="'+w+'"');if(0!=C.rowCount){var L=Titanium.UI.createImageView({image:"/images/has_offers.png",height:25,width:73,left:5,touchEnabled:!1});I.add(L)}return F.close(),s.add(I),s}module.exports=featuredResultsFormat;