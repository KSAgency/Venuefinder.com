function featuredResultsFormat(e,t,i,a,n,o,r,l,d,s){if(null!=s||"iPad"!=Ti.App.Properties.getString("osname"))var u=Titanium.UI.createTableViewRow({height:150,venueTitle:e,uniqueID:t,selectedBackgroundColor:"#FFF",backgroundColor:"#FFF"});else var u=Titanium.UI.createView({height:150,width:327,left:10,top:10,venueTitle:e,uniqueID:t,backgroundColor:"#FFF",borderRadius:10,borderColor:"#999"});var c=Titanium.UI.createView({width:96,left:5,height:87,top:10,touchEnabled:!1});"Android"!=Ti.App.Properties.getString("osname")&&c.setLeft(15);var f=Titanium.UI.createImageView({image:"http://www.venuefinder.com/gallery/"+i,defaultImage:"/images/icon.png",hires:!0,width:140,touchEnabled:!1});null==i&&f.setImage("/images/icon.png");var m=Titanium.UI.createActivityIndicator();"Android"!=Ti.App.Properties.getString("osname")&&m.setStyle(Titanium.UI.iPhone.ActivityIndicatorStyle.DARK),c.add(m),m.show(),f.addEventListener("load",function(){f.setWidth(280),m.hide()});var g=Titanium.UI.createImageView({image:"/images/featuredbanner.png",touchEnabled:!1,top:0,left:0});null!=d&&""!=d&&g.setImage(d),c.add(f),c.add(g),u.add(c);var p=Titanium.UI.createView({width:190,left:120,height:45,top:5,touchEnabled:!1}),h=Titanium.UI.createLabel({text:e,width:Ti.UI.SIZE,height:Ti.UI.SIZE,top:0,left:0,ellipsize:!0,color:"#2195be",font:{fontSize:18,fontWeight:"bold"},touchEnabled:!1});"Android"!=Ti.App.Properties.getString("osname")&&(p.setLeft(125),p.setWidth(180));var T=Titanium.UI.createLabel({text:r+l,left:120,width:190,height:30,top:43,ellipsize:!0,color:"#A2BE1C",font:{fontSize:16},touchEnabled:!1});"Android"!=Ti.App.Properties.getString("osname")&&(T.setLeft(125),T.setWidth(180)),(null==o||""==o)&&(o="No description could be retrieved for this venue.");var b=Titanium.UI.createLabel({text:o,left:120,width:190,height:35,top:65,touchEnabled:!1,color:"#666",font:{fontSize:14},touchEnabled:!1});"Android"!=Ti.App.Properties.getString("osname")?(b.setLeft(125),b.setWidth(180)):b.setHeight(34),e.length<20&&(h.setHeight(20),T.setTop(20),b.setTop(43),b.setHeight("Android"!=Ti.App.Properties.getString("osname")?55:52)),p.add(h),u.add(p),u.add(T),u.add(b);var v=Ti.UI.createView({height:25,width:Ti.UI.SIZE,top:110,left:0,layout:"horizontal",touchEnabled:!1});if("Android"!=Ti.App.Properties.getString("osname")&&v.setLeft(10),null!=a&&a>=0){var I=Titanium.UI.createImageView({image:"/images/has_bedrooms.png",height:25,width:25,left:5,touchEnabled:!1});v.add(I)}if(1==n){var S=Titanium.UI.createImageView({image:"/images/has_access.png",height:25,width:25,left:5,touchEnabled:!1});v.add(S)}var w=new Date,E=w.getDate(),A=w.getMonth()+1,y=w.getFullYear();10>E&&(E="0"+E),10>A&&(A="0"+A),w=y+"-"+A+"-"+E;var U=require("/builders/databaseFunctions/createDatabase"),F=U("/venuefinder.db","venuefinder"),C=F.execute('SELECT * FROM Offers WHERE EntryID="'+t+'" AND validToDate>="'+w+'"');if(0!=C.rowCount){var L=Titanium.UI.createImageView({image:"/images/has_offers.png",height:25,width:73,left:5,touchEnabled:!1});v.add(L)}return F.close(),u.add(v),u}module.exports=featuredResultsFormat;