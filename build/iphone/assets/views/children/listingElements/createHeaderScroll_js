function headerScroll(e,t){var i=-1,n=null,a=require("/builders/databaseFunctions/createDatabase"),r=a("/venuefinder.db","venuefinder"),s=r.execute('SELECT * FROM Venue WHERE VenueID = "'+t+'"'),o=s.fieldByName("PackageCode");if("FRE"==o){var l=Titanium.UI.createView({width:1026,height:200,top:-1,borderColor:"#999",borderWidth:1}),d=Ti.UI.createImageView({image:"/images/logoIpad.png",height:150});l.add(d)}else var l=Titanium.UI.createScrollView({contentWidth:Ti.UI.SIZE,contentHeight:200,width:1026,height:200,top:-1,layout:"horizontal",borderColor:"#999",borderWidth:1});if("FRE"!=o)for(var c=[],a=require("/builders/databaseFunctions/createDatabase"),r=a("/venuefinder.db","venuefinder"),u=r.execute('SELECT * FROM VenueAdvertOptionsForWeb WHERE VenueID="'+t+'" ORDER BY OptionCode DESC');u.isValidRow();){var f=u.fieldByName("OptionCode"),p=u.fieldByName("GraphicFileName"),g=u.fieldByName("URL");if("TOP"==f||"PIC"==f||"TOP"==f||"MID"==f||"MIL"==f||"MIR"==f){n=i++;var m=Titanium.UI.createView({width:190,height:190,left:10}),h=Titanium.UI.createImageView({image:"http://www.venuefinder.com/gallery/"+p,defaultImage:"/images/icon.png",imageCode:p,index:i,width:1024}),v=h.toImage();v.width>=v.height?h.setWidth(1024):h.setHeight(1024),c.push(h.image),v.width>=v.height?h.setWidth(400):h.setHeight(400),m.add(h),l.add(m),h.addEventListener("click",function(t){{var i=require("/builders/createImageGallery");i(e,c,t.source.index)}})}if("VID"==f){var b=Titanium.UI.createView({width:180,height:180,left:10,video:g}),T=(g.substring(29,60),Titanium.UI.createImageView({image:"http://www.venuefinder.com/gallery/"+p,defaultImage:"/images/icon.png",touchEnabled:!1,width:370,height:200})),w=Titanium.UI.createImageView({image:"/images/videobanner.png",touchEnabled:!1,zIndex:5,top:0,left:0}),I=Titanium.UI.createImageView({image:"/images/playButton.png",touchEnabled:!1,zIndex:4,top:40,width:100,height:100});b.addEventListener("click",function(t){var i=require("/builders/createApplicationWindow"),