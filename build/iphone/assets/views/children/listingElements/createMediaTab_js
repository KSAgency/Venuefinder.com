E);var S=Titanium.UI.createScrollView({contentWidth:320,contentHeight:Ti.UI.FILL,width:Ti.UI.FILL,height:"160",top:"5",verticalBounce:!1,horizontalBounce:!1,layout:"horizontal",showHorizontalScrollIndicator:!0});i.add(S);var _=Titanium.UI.createLabel({text:"Please click here to watch this video",backgroundColor:"#A2BE1C",color:"#FFF",height:30,width:300,textAlign:Ti.UI.TEXT_ALIGNMENT_CENTER,left:10,font:{fontSize:"16",fontFamily:"Arial"}});if(_.addEventListener("click",function(){Titanium.Platform.openURL(x)}),"Android"!=Ti.App.Properties.getString("osname")){var A=Titanium.UI.createWebView({url:x,width:300,left:10,height:Ti.UI.FILL});S.add(A)}else S.add(_)}v.next()}}else tab2.setBackgroundImage("/images/photos_media_ns.png")}else{var s=require("/builders/databaseFunctions/createDatabase"),l=s("/venuefinder.db","venuefinder"),L=l.execute('SELECT * FROM Venue WHERE VenueID="'+a+'"'),k=L.fieldByName("PackageCode"),F="No description could be found for this venue.";if("PRE"==k){var V=l.execute('SELECT Text FROM VenueAdvertOptionsForWeb WHERE VenueID="'+a+'" AND OptionCode="SG2"');F=V.fieldByName("Text")}else{var V=l.execute('SELECT DescriptionText FROM VenueTextForWeb WHERE VenueID="'+a+'"');F=V.fieldByName("DescriptionText")}(null==F||""==F)&&(F="No description could be retrieved for this venue."),F.replace("/<(?:.|\n)*?>/gm",""),l.close();var U=Titanium.UI.createWebView({html:'<span style="font-family:Arial; color:#666; font-size:14px; line-height:20pts; font-size:15px;">'+F+"</span>",top:"20",width:"95%"});i.add(U)}}module.exports=mediaTab;
