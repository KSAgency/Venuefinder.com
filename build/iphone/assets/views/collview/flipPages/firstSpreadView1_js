function firstSpreadView1(e,t){Ti.API.info("** venue detail "+venueNameDetail);var i=venueNameDetail.getVenueAdvert(t),a=Ti.UI.createView({width:"382",height:"540",top:"76",left:"44",link:i[1]}),n=Ti.UI.createImageView({image:i[0],touchEnabled:!1});a.addEventListener("click",function(e){var t=Titanium.UI.createWindow({backgroundColor:"rgba(0,0,0,0.8)"}),i=Titanium.UI.createButton({right:"5%",top:"5%",height:"20",zIndex:1,title:"X",color:"#FFF",font:{fontSize:"28",fontWeight:"bold"}});t.add(i),i.addEventListener("click",function(){t.close()});var a=Ti.UI.createView({width:"80%",height:"80%",top:"10%",left:"10%"}),n=Titanium.UI.createWebView({url:e.source.link});a.add(n),t.add(a),t.open()});var r=n.toImage();if(r.width>=r.height){var o=r.height/a.height;n.setHeight(a.height),n.setWidth(r.width/o)}else{var o=r.width/a.width;n.setHeight(a.width),n.setWidth(r.height/o)}return a.add(n),e.add(a),e}var venueNameDetail=require("/views/collview/flipPages/getVenueNameAndDetail");module.exports=firstSpreadView1;