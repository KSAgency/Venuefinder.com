function SearchResults(e,t,i,a,n){for(var r=[],o=[],s=require("/builders/databaseFunctions/createDatabase"),l=s("/venuefinder.db","venuefinder"),d=0;d<i.length;d++)for(var u=l.execute(i[d]);u.isValidRow();){if(void 0!=u.fieldByName("VenueName")){var c=0,f=0,p=[];p.venueID=u.fieldByName("VenueID"),p.venueName=u.fieldByName("VenueName"),p.venueTown=u.fieldByName("Town"),p.bedroomsNo=u.fieldByName("BedroomsNo"),p.postCode=u.fieldByName("Postcode"),p.meetingRoomsNo=u.fieldByName("MeetingRoomsNo"),p.packageCode=u.fieldByName("PackageCode");var g=l.execute('SELECT * FROM VenueAdvertOptionsForWeb WHERE VenueID="'+p.venueID+'" AND (OptionCode="TOP" OR OptionCode="PIC")'),m="";g.isValidRow()&&(m=g.fieldByName("GraphicFileName")),p.imageUrl=m;var h="";""!=p.venueTown?h=""!=p.postCode?p.venueTown+", "+p.postCode:p.venueTown:""!=p.postCode&&(h=p.postCode),p.location=h,o.push(p)}u.next()}l.close();var v=require("/views/collview/getVenueThumbView");if(0==o.length)return"NoRecord";for(var b,s=require("/builders/databaseFunctions/createDatabase"),l=s("/venuefinder.db","venuefinder"),u=l.execute("select VenueStyle from VenueStyles where VenueStyleID = "+a);u.isValidRow();)b=u.fieldByName("VenueStyle"),u.next();l.close();var T=o.length;if(0!=T){for(var w=Math.ceil(T/12),I=0,d=0;w>d;d++){var y=v.setWindowPageStyle(b,!1);r.push(y[0]);var S=Titanium.UI.createImageView({image:"/images/has_bedrooms.png",height:"21",width:"22",left:"718",top:"12"});y[0].add(S);var A=Ti.UI.createLabel({text:"Bedrooms",top:"17",left:"744",color:"#000000",width:"60",height:"9",font:{fontSize:12,fontFamily:Ti.App.Properties.getString("fontFamily")}});y[0].add(A);var E=Titanium.UI.createImageView({image:"/images/meeting_room.png",height:"21",width:"22",left:"814",top:"12"});y[0].add(E);var V=Ti.UI.createLabel({text:"Meeting rooms",top:"17",left:"842",color:"#000000",width:"100",height:"9",font:{fontSize:12,fontFamily:Ti.App.Properties.getString("fontFamily")}});y[0].add(V);for(var c=80,C=11,L=11,F=84,x=0;3>x;x++){for(var f=80,U=0;4>U;U++){var N=o[I];if(void 0!=N&&void 0!=N.venueName){2==U&&(f=f+F-L);var k=0;if("BRZ"==N.packageCode)for(var x=I+1;x<o.length;x++)if(void 0!=o[x].packageCode&&"BRZ"==o[x].packageCode){k=o[x].venueID;break}var D=v.getVenueThumb(o[I],c,f,n,k,!1,!0);y[0].add(D),f=f+168+L}I++}c=c+178+C}}var P=Ti.UI.createScrollableView({views:r,currentPage:0,backgroundColor:"#ffffff",top:"41",width:"938",height:"660",cacheSize:0});t.add(P),lastView=r[0];{var R=require("views/collview/searchMapChild/SliderViewCreation");R(t,T,w,lastView,r,P)}}}module.exports=SearchResults;